<!DOCTYPE html>
<html lang="en">
<head>
  <title>Document</title>
</head>
<body>
  列表页面
  <ul>
    <% photos.forEach(function(item){ %>
    <li><%= item.name %></li>
    <a href='/photo/<%= item.id %>/download'>
      <img src='/photos/<%= item.path %>' alt="">
    </a>
    <% }) %>
  </ul>
  <a href="./upload">跳转</a>
</body>
</html>